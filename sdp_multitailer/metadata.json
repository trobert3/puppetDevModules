{
  "name": "vodafone-sdp_multitailer",
  "version": "1.0.0",
  "author": "robert.tingirica@vodafone.com",
  "summary": "Install and configure multitailer application",
  "license": "Vodafone",
  "source": "https://svn.sp.vodafone.com/git/sdp_puppet_modules.git",
  "project_page": "https://confluence.sp.vodafone.com/display/ESOR/SDP_MULTITAILER",
  "issues_url": "https://jira.sp.vodafone.com/browse/ETSOSD-11907",
  "dependencies": [
    {
        "name": "puppet-archive",
        "version_requirement": ">= 0.5.1"
    }
    {
        "name": "puppetlabs-stdlib",
        "version_requirement": ">= 4.12.0"
    },
    {
        "name": "vodafone_java",
        "version_requirement": ">= 1.0.0"
    },
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
      {
          "operatingsystem": "RedHat",
          "operatingsystemrelease": [
              "6.0",
              "7.0"
          ]
      },
      {
           "operatingsystem": "CentOS",
           "operatingsystemrelease": [
               "6",
               "7"
           ]
      },
      {
           "operatingsystem": "Solaris",
           "operatingsystemrelease": [
               "10",
               "11"
           ]
      }
  ],
  "requirements": [
      {
          "name": "pe",
          "version_requirement": ">= 4.5.0"
      },
      {
          "name": "puppet",
          "version_requirement": ">= 4.5.0 < 5.0.0"
      }
  ]
}
